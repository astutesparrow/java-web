<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>Employees</title>
</head>
<body>
<table border=1>
	<thead><tr>
		<th>ID</th>
		<th>Name</th>
		<th>Email</th>
	</tr></thead>
	#foreach( $employee in $employees.employees)
	    <tr>
			<td>${employee.id}</td>
			<td>${employee.name}</td>
			<td>${employee.email}</td>
		</tr>
	#end
</table>
</body>
</html>